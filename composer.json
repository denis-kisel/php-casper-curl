{
    "name": "denis-kisel/casper-curl",
    "description": "A phantomjsCURL for get content of difficult sites",
    "license": "MIT",
    "keywords": [
        "phantomjs",
        "laravel",
        "curl"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Denis Kisel",
            "email": "denis.kisel92@gmail.com",
            "homepage": "http://digital-elephant.com.ua"
        }
    ],
    "homepage": "https://github.com/denis-kisel/php-casper-curl",
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DenisKisel\\CasperCURL\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenisKisel\\CasperCURL\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
